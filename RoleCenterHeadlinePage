    hdl3Txt: Label 'This is headline 3';
    hdl4Txt: Label 'This is headline 4';
}


field(Headline1; hdl1Txt)
{
    trigger OnDrillDown()
    var
        DrillDownURLTxt: Label 'https://go.microsoft.com/fwlink/?linkid=867580', Locked = True;
    begin
        Hyperlink(DrillDownURLTxt)
    end;
}


{
